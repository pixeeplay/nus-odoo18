<odoo>
<data noupdate="1">
    <record id="ir_cron_prestashop_sync" model="ir.cron">
        <name>PrestaShop: Hourly Order Sync</name>
        <model_id ref="model_prestashop_instance"/>
        <state>code</state>
        <code>model._cron_sync_orders()</code>
        <user_id ref="base.user_root"/>
