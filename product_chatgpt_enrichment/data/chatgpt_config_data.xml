<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        
        <!-- Default ChatGPT Configuration -->
        <!-- Default AI Configuration -->
        <record id="default_chatgpt_config" model="chatgpt.config">
            <field name="name">Default OpenAI Config</field>
            <field name="provider">openai</field>
            <field name="ai_model_name">gpt-4o-mini</field>
            <field name="api_key">YOUR_API_KEY_HERE</field>
            <field name="api_endpoint">/v1/chat/completions</field>
            <field name="auto_enrich" eval="False"/>
            <field name="max_tokens">1000</field>
            <field name="temperature">0.7</field>
            <field name="active" eval="True"/>
        </record>

    </data>
</odoo>
